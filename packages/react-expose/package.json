{
  "name": "react-mission-control",
  "version": "1.0.0",
  "type": "module",
  "description": "macOS Mission Control-like experience for React applications",
  "publishConfig": {
    "access": "public"
  },
  "author": "Ivan Husarov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Houstoten/react-mission-control"
  },
  "homepage": "https://github.com/Houstoten/react-mission-control#readme",
  "bugs": {
    "url": "https://github.com/Houstoten/react-mission-control/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css",
    "./css": "./dist/index.css",
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "clean": "rm -rf dist .turbo"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsdown": "^0.14.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "react",
    "mission-control",
    "expose",
    "ui",
    "animation",
    "macos",
    "grid",
    "overview"
  ],
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "html-to-image": "^1.11.13",
    "zustand": "^5.0.8"
  }
}
